<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#786B6B"/>
            <corners android:radius="2dp" />
        </shape>
    </item>

    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1.25dp"
        android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/light_cream_brown"/>
            <corners android:radius="2dp" />
        </shape>
    </item>
</layer-list>